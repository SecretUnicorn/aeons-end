    "@mui/material": "^5.10.6",
    "@mui/system": "^5.10.6",
    "@mui/utils": "^5.10.6",
    "@mui/x-date-pickers": "^5.0.9",
    "axios": "^1.2.1",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "html-react-parser": "^3.0.4",
    "is-mobile": "^3.1.1",
    "luxon": "^3.1.1",
    "notistack": "^2.0.5",
